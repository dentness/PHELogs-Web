<div>
    <form class="form-inline" onsubmit="add_entry()">
        <div class="form-group">
            <label for="food">Food:</label>
            <input type="email" class="form-control" id="food">
        </div>
        <div class="form-group">
            <input type="number" class="form-control" id="grams">
            <label for="grams">grams</label>
        </div>
        <div class="form-group">
            <input type="text" class="form-control" id="calcPHe">
            <label for="calcPhe">mgPHE/g</label>
        </div>
        <div class="form-group">
            <input type="date" class="form-control" id="entryDate">
            <label for='entryDate'> <i class="fas fa-calendar"></i></label>
        </div>
        <button type="submit" class="btn btn-default">Add</button>
    </form>
</div>
