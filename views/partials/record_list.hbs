<div>
    <table class="table">
        <thead class="thead-dark">
        <tr>
            <th scope="col">Date</th>
            <th scope="col">Goal</th>
            <th scope="col">Final</th>
            <th scope="col">Notes</th>
            <th>&nbsp;</th>
        </tr>
        </thead>
        <tbody>
        {{#each records}}
            <tr>
                <td>{{ date }}</td>
                <td>{{ goal }}</td>
                <td>{{ final }}</td>
                <td>{{ notes }}</td>
                <td><a href="/records/details/{{ id }}">View</a></td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>
